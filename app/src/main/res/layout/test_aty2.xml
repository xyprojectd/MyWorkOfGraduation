<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="horizontal"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#FDF5E6">



    <Button
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:text="插入"
        android:textSize="17dp"
        android:textColor="#6F000000"
        android:id="@+id/btnInsert"
        android:background="@drawable/returnshape"
        android:layout_marginTop="23dp"
        android:layout_marginLeft="10dp"
        android:layout_alignParentTop="true"

        />


    <EditText
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:id="@+id/editTextDeletePosition"
        android:background="@drawable/edittext"
        android:textSize="15dp"
        android:hint="@string/删除结点"
        android:textColor="#FF000000"
        android:layout_marginTop="23dp"
        android:layout_below="@+id/btnDelete"
        android:layout_alignLeft="@+id/btnDelete"
        android:layout_alignStart="@+id/btnDelete" />

    <Button
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:text="删除"
        android:textSize="17dp"
        android:textColor="#6F000000"
        android:id="@+id/btnDelete"
        android:background="@drawable/returnshape"
        android:layout_gravity="right|top"
        android:layout_below="@+id/editTextInsertPosition"
        android:layout_alignLeft="@+id/editTextInsertPosition"
        android:layout_alignStart="@+id/editTextInsertPosition"
        android:layout_marginTop="28dp" />

    <EditText
        android:layout_width="85dp"
        android:layout_height="40dp"
        android:id="@+id/editTextInsertPosition"
        android:background="@drawable/edittext"
        android:textSize="15dp"
        android:hint="@string/输入位置"
        android:textColor="#FF000000"
        android:layout_gravity="center_horizontal|top"
        android:layout_marginTop="27dp"
        android:layout_below="@+id/btnInsert"
        android:layout_alignLeft="@+id/btnInsert"
        android:layout_alignStart="@+id/btnInsert" />

    <TextView
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" Listnode1|   "
        android:id="@+id/tv1"
        android:layout_marginLeft="15dp"
        android:layout_marginStart="22dp"
        android:background="@drawable/list"
        android:layout_below="@+id/btnInsert"
        android:layout_toRightOf="@+id/btnInsert"
        android:layout_toEndOf="@+id/btnInsert" />

    <TextView
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" Listnode2 |   "
        android:id="@+id/tv2"
        android:background="@drawable/list"
        android:layout_marginLeft="30dp"
        android:layout_marginStart="34dp"
        android:layout_alignTop="@+id/tv1"
        android:layout_toRightOf="@+id/tv1"
        android:layout_toEndOf="@+id/tv1" />

    <TextView
        android:layout_width="145dp"
        android:layout_height="30dp"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" Listnode3 |   "
        android:background="@drawable/list"
        android:id="@+id/tv3"
        android:layout_alignTop="@+id/tv2"
        android:layout_alignParentRight="true"
        android:layout_marginRight="10dp"/>

    <TextView
        android:layout_width="140dp"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=" Listnode4 |   "
        android:background="@drawable/list"
        android:id="@+id/tv4"
        android:layout_alignTop="@+id/btnDelete"
        android:layout_alignLeft="@+id/tv1"
        android:layout_alignStart="@+id/tv1"
        android:layout_marginLeft="81dp"
        android:layout_marginStart="81dp" />


    <Button
        android:layout_width="90dp"
        android:layout_height="40dp"
        android:text="还原"
        android:textSize="17dp"
        android:textColor="#6F000000"
        android:id="@+id/btnRecover"
        android:background="@drawable/returnshape"
        android:layout_alignBottom="@+id/editTextDeletePosition"
        android:layout_alignLeft="@+id/tv3"
        android:layout_alignStart="@+id/tv3"
        android:layout_marginLeft="26dp"
        android:layout_marginStart="26dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text=""
        android:id="@+id/textView24"
        android:layout_alignBottom="@+id/editTextInsertPosition"
        android:layout_alignLeft="@+id/tv4"
        android:layout_alignStart="@+id/tv4" />

    <ImageView
        android:layout_width="80dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView1"
        android:src="@drawable/img1"
        android:layout_above="@+id/textView24"
        android:layout_toRightOf="@+id/textView24"
        android:layout_toEndOf="@+id/textView24"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp" />

    <ImageView
        android:layout_width="65dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView2"
        android:src="@drawable/img1"
        android:layout_marginRight="-20dp"
        android:layout_alignTop="@+id/imageView1"
        android:layout_toLeftOf="@+id/btnRecover"
        android:layout_toStartOf="@+id/btnRecover" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView3"
        android:src="@drawable/img2"
        android:layout_alignBottom="@+id/tv4"
        android:layout_toLeftOf="@+id/tv2"
        android:layout_toStartOf="@+id/tv2" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:id="@+id/imageView4"
        android:src="@drawable/img3"
        android:layout_alignBottom="@+id/tv4"
        android:layout_marginLeft="-10dp"
        android:layout_marginBottom="4dp"
        android:layout_toRightOf="@+id/imageView1"
        android:layout_toEndOf="@+id/imageView1" />

    <ImageView
        android:layout_width="270dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView5"
        android:src="@drawable/img4"
        android:layout_alignBottom="@+id/tv1"
        android:layout_marginTop="0dp"
        android:layout_alignLeft="@+id/imageView1"
        android:layout_alignStart="@+id/imageView1" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="头指针"
        android:background="@drawable/list"
        android:textSize="20dp"
        android:id="@+id/tvHead"
        android:layout_above="@+id/tv4"
        android:layout_alignLeft="@+id/ivHead"
        android:layout_alignStart="@+id/ivHead" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="60dp"
        android:id="@+id/ivHead"
        android:src="@drawable/head"
        android:layout_above="@+id/tvHead"
        android:layout_alignLeft="@+id/tv1"
        android:layout_alignStart="@+id/tv1"
        android:layout_marginBottom="-5dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imageView6"
        android:src="@drawable/img5"
        android:layout_alignBottom="@+id/tv4"
        android:layout_alignRight="@+id/tv4"
        android:layout_alignEnd="@+id/tv4"
        android:layout_marginRight="7dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="40dp"
        android:id="@+id/ivHeadTo4"
        android:src="@drawable/img6"
        android:layout_below="@+id/tvHead"
        android:layout_marginTop="-3dp"
        android:layout_toLeftOf="@+id/imageView5"
        android:layout_toStartOf="@+id/imageView5" />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="110dp"
        android:id="@+id/NULL"
        android:src="@drawable/img2"
        android:layout_alignBottom="@+id/imageView4"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginRight="4dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="NULL"
        android:textSize="20dp"
        android:background="@drawable/list"
        android:id="@+id/tvNull"
        android:layout_alignTop="@+id/ivHeadTo4"
        android:layout_alignRight="@+id/NULL"
        android:layout_alignEnd="@+id/NULL"
        android:layout_marginRight="5dp"/>

    <ImageView
        android:layout_width="240dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView7"
        android:src="@drawable/img7"
        android:layout_alignBottom="@+id/ivHeadTo4"
        android:layout_alignRight="@+id/btnRecover"
        android:layout_alignEnd="@+id/btnRecover" />

    <ImageView
        android:layout_width="340dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView8"
        android:src="@drawable/img8"
        android:layout_alignTop="@+id/imageView6"
        android:layout_alignRight="@+id/tv3"
        android:layout_alignEnd="@+id/tv3"
        android:layout_marginRight="15dp"/>

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="70dp"
        android:id="@+id/headToNode2"
        android:src="@drawable/img9"
        android:layout_alignTop="@+id/imageView3"
        android:layout_alignRight="@+id/tv4"
        android:layout_alignEnd="@+id/tv4" />

    <ImageView
        android:layout_width="175dp"
        android:layout_height="wrap_content"
        android:id="@+id/imageView10"
        android:src="@drawable/img10"
        android:layout_alignTop="@+id/imageView2"
        android:layout_alignLeft="@+id/imageView2"
        android:layout_alignStart="@+id/imageView2" />


</RelativeLayout>